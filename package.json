{
  "name": "@ajainarayanan/react-pan-zoom",
  "version": "0.0.3",
  "description": "Simple wrapper for pan and zoom on html containers",
  "main": "dist/react-pan-zoom",
  "typings": "dist/react-pan-zoom.d.ts",
  "author": "ajainarayanan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ajainarayanan/react-pan-zoom.git"
  },
  "keywords": [
    "html pan",
    "html zoom",
    "react pan zoom"
  ],
  "scripts": {
    "start:dev": "webpack-dev-server",
    "lib:build": "webpack --config=webpack.config.lib.js",
    "test": "jest --watchAll --coverage",
    "itest": "cypress open",
    "prepublishOnly": "npm run lib:build"
  },
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.19",
    "cypress": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.1.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "styled-components": "^3.2.6",
    "svg-sprite-loader": "^3.7.3",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.3.0",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ],
    "coverageReporters": [
      "html"
    ]
  }
}
